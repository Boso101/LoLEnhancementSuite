<UserControl x:Class="LESs.ByteLabel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LESs"
             mc:Ignorable="d" 
             d:DesignHeight="26" d:DesignWidth="300">
    <Grid>
        <TextBox x:Name="ByteTextBox" HorizontalAlignment="Left" Height="30" Margin="2, -2, 0, 0" VerticalContentAlignment="Center" TextWrapping="Wrap" Text="255" VerticalAlignment="Top" Width="50" KeyDown="ByteTextBox_KeyDown" PreviewTextInput="ByteTextBox_PreviewTextInput" LostFocus="ByteTextBox_LostFocus" TextChanged="ByteTextBox_TextChanged" FontSize="14.667"/>
    </Grid>
</UserControl>
